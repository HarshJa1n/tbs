{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "react": [ "./node_modules/@types/react" ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "renderer/next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/api/medium-feed.js"  ]
}
